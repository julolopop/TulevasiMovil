<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.julolopop.tulevasi">

    <application
        android:name=".TulevasiApplication"
        android:allowBackup="true"
        android:icon="@drawable/tulevasi"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.login.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.pref.PreferentLogin" />
        <activity android:name=".ui.pref.Prefrentzona" />
        <activity android:name=".ui.login.MainActivity" />
        <activity android:name=".ui.MesasActivity" />
        <activity android:name=".about.AboutActivity" />

        <receiver android:name=".broadcast.TulevasiBroadcast">
            <intent-filter>
                <action android:name="com.example.julolopop.tulevasi" />
            </intent-filter>
        </receiver>

        <activity android:name=".ImpresionActivity"></activity>
    </application>

</manifest>